<main id="main">
  <div class="inner-banner" style="min-height: auto; background-color:#124549">
    <div class="container text-center">
      <h1>404 Not Found</h1>
    </div>
  </div>
  <div class="container" style="display:table; min-height: 300px;">
    <div style="display:table-cell; vertical-align: middle; width: auto">
      <h4 style="text-align: center">
        <% if (typeof data !== 'undefined') { %>
        <%= data %>
        <% } else { %>
        The page you were trying to reach doesn't exist.
        <% } %>
      </h4>
    </div>
  </div>
</main>
